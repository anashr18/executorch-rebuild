{
  "name": "executorch-dev (template)",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/work",
  "runArgs": ["--init"],
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "ms-vscode.cmake-tools",
        "rust-lang.rust-analyzer"
      ]
    }
  },
  "postCreateCommand": "cmake --version && buck2 --version"
}

